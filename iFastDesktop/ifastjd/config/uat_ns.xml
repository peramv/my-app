<?xml version="1.0" ?>
<Server name="NetServer">

  <Parameters>
    <defaultHost>AWD</defaultHost>
    <defaultWorkstation>NETSRVWS</defaultWorkstation>
    <controlPort></controlPort>
    <xRMIRegistryPort>1099</xRMIRegistryPort>
    <startRMIRegistry>No</startRMIRegistry>
    <errorNode>error</errorNode>
    <trace>24571</trace>
    <!--<xinitTrace>24571</xinitTrace>-->
  </Parameters>

  <JobManager>
    <desc>Decision Engine Job Manager</desc>
    <class>com.dstsystems.awd.ns2.jobManager.de.deJobManager</class>
    <host>AWD</host>
    <userID>AWDBATCH</userID>
    <appName_Admin>dnadmin</appName_Admin>
  </JobManager>

  <HostManager>
    <class>com.dstsystems.awd.ns2.hostManager.HostManager</class>
  </HostManager>

  <Logger>
    <name>Local Console Logger</name>
    <class>com.dstsystems.awd.util.consoleLogger</class>
  </Logger>

  <!-- AWD Select -->
  <Load>
    <name>RulesEngineNSService</name>
    <desc>Rules Engine Loader</desc>
    <class>com.dstsystems.awd.ns2.jobManager.ke.tasks.RulesEngineNSServiceSingleton</class>
    <rulesPath>/wdcapps/J2EE/CanadaTest/apps/ifdeskuatEar/ifdeskuatWebApp/ifdesk/uat/config/rules/</rulesPath>
    <businessProperties>/wdcapps/J2EE/CanadaTest/apps/ifdeskuatEar/ifdeskuatWebApp/ifdesk/uat/config/rules/BusinessProperties.xml</businessProperties>
    <businessDataClassName>com.dsti.rulesengine.BusinessData.BusinessData</businessDataClassName>
    <rulesEngineConfigurationPath>/wdcapps/J2EE/CanadaTest/apps/ifdeskuatEar/ifdeskuatWebApp/ifdesk/uat/config/AWDSelectConfiguration.xml</rulesEngineConfigurationPath>
  </Load>
  <!-- AWD Select -->

  <de>
    <objecttable>
      <com.dstsystems.de.Job>J</com.dstsystems.de.Job>
      <com.dstsystems.de.ProcessSteps>PS</com.dstsystems.de.ProcessSteps>
      <com.dstsystems.de.ProcessStep>P</com.dstsystems.de.ProcessStep>
      <com.dstsystems.de.TaskDefs>TS</com.dstsystems.de.TaskDefs>
      <com.dstsystems.de.TaskDef>T</com.dstsystems.de.TaskDef>
      <com.dstsystems.de.Links>LS</com.dstsystems.de.Links>
      <com.dstsystems.de.Link>L</com.dstsystems.de.Link>
    </objecttable>
    <loggers>
      <logger name="NetServerLogger">
        <class>com.dstsystems.awd.ns2.jobManager.de.deLogAdapter</class>
        <configinfo/>
      </logger>
    </loggers>
    <loaders>
      <loader>
        <app>NETSERVER</app>
        <class>com.dstsystems.awd.ns2.jobManager.de.ViewLoader</class>
        <host>AWD</host>
        <userID>AWDBATCH</userID>
        <configinfo>
          <![CDATA[]]>
        </configinfo>
        <default/>
      </loader>
      <loader>
        <app>dnadmin</app>
        <class>com.dstsystems.de.loaders.JavaLoader</class>
      </loader>

      <loader>
        <app>client</app>
        <class>com.dstsystems.de.contact.loaders.XMLFileLoader</class>
        <configinfo><![CDATA[/wdcapps/J2EE/CanadaTest/apps/ifdeskuatEar/ifdeskuatWebApp/ifdesk/uat/client/jobs]]></configinfo>
      </loader>

      <loader>
        <app>uataim</app>
        <class>com.dstsystems.de.contact.loaders.XMLFileLoader</class>
        <configinfo><![CDATA[/wdcapps/J2EE/CanadaTest/apps/ifdeskuatEar/ifdeskuatWebApp/ifdesk/uat/uataim/jobs]]></configinfo>
      </loader>

      <loader>
        <app>uatprod</app>
        <class>com.dstsystems.de.contact.loaders.XMLFileLoader</class>
        <configinfo><![CDATA[/wdcapps/J2EE/CanadaTest/apps/ifdeskuatEar/ifdeskuatWebApp/ifdesk/uat/uatprod/jobs]]></configinfo>
      </loader>

     <loader>
        <app>uatprdaim</app>
        <class>com.dstsystems.de.contact.loaders.XMLFileLoader</class>
        <configinfo><![CDATA[/wdcapps/J2EE/CanadaTest/apps/ifdeskuatEar/ifdeskuatWebApp/ifdesk/uat/uatprdaim/jobs]]></configinfo>
      </loader>
     <loader>
        <app>ssbmo</app>
        <class>com.dstsystems.de.contact.loaders.XMLFileLoader</class>
        <configinfo><![CDATA[/wdcapps/J2EE/CanadaTest/apps/ifdeskuatEar/ifdeskuatWebApp/ifdesk/uat/ssbmo/jobs]]></configinfo>
      </loader>



    </loaders>
    <messageformats>
      <messageformat>
        <key>SETS</key>
        <format>DE: {0} - setting value of ''{1}'' to ''{2}''.</format>
      </messageformat>
      <messageformat>
        <key>GETS</key>
        <format>DE: {0} - getting from ''{1}'' the value of ''{2}''.</format>
      </messageformat>
      <messageformat>
        <key>WNOD</key>
        <format>DE: {0} - the node, {1}, and is invalid for this object.</format>
      </messageformat>
      <messageformat>
        <key>ENTR</key>
        <format>DE: {0} - entering method ''{1}''"</format>
      </messageformat>
      <messageformat>
        <key>EXIT</key>
        <format>DE: {0} - exiting method ''{1}''</format>
      </messageformat>
      <messageformat>
        <key>NULL</key>
        <format>DE: {0}.{1} - the object ''{2}'' is null.</format>
      </messageformat>
      <messageformat>
        <key>ERRR</key>
        <format>DE: {0}.{1}  ---------ERROR---------  The following error was thrown - {2}</format>
      </messageformat>
      <messageformat>
        <key>DBUG</key>
        <format>DE: {0}.{1} - {2}</format>
      </messageformat>
      <messageformat>
        <key>EXEC</key>
        <format>DE: {0}.{1} - {1}</format>
      </messageformat>
      <messageformat>
        <key>WBB4</key>
        <format>DE: {0}.{1} WB BEFORE execution: \n{2}</format>
      </messageformat>
      <messageformat>
        <key>WBAF</key>
        <format>DE: {0}.{1} WB AFTER execution: \n{2}</format>
      </messageformat>
      <messageformat>
        <key>PERF</key>
        <format>{0}</format>
      </messageformat>
    </messageformats>
    <traces>
    </traces>
    <trace>StartUp</trace>
    <trace>ViewErrors</trace>
    <trace>ViewParse</trace>
    <trace>Config</trace>
    <trace>DEBUG</trace>
    <trace>JobFlow</trace>
    <loadmanager>
      <tracepropagation>true</tracepropagation>
    </loadmanager>
  </de>

  <IFDS>
    <!--
      - New Sonic7.6 Config
      -->
    <MessageConfig>
      <ConfigAddress>tcp://10.188.52.66:30106,tcp://10.188.52.108:30106</ConfigAddress>
      <ConfigQueue>ConfigQueryRequest</ConfigQueue>
      <ConfigApplicationType>DESKTOP</ConfigApplicationType>
      <ConfigLocationId>extern-kc</ConfigLocationId>  - <!-- internal or external, external-kc -->
      <ConfigUsername>ifastdesktop</ConfigUsername>
      <ConfigPassword>ifastdesktop2468</ConfigPassword>
      <ConfigRefreshMillis>3600000</ConfigRefreshMillis> - <!-- 1 hour refresh of config -->
      <ConfigCacheLocation></ConfigCacheLocation>
      <MessageTimeToLive>60000</MessageTimeToLive>
    </MessageConfig>

    <SmartPDFConfig>
      <dataPath>dsk_data/uat/datadir</dataPath>
      <commonDirectory>client</commonDirectory>
      <configDirectory>xml</configDirectory>
      <imageDirectory>images/taxForms</imageDirectory>
      <resourcesDirectory>resources</resourcesDirectory>
      <decimalDelimitor_en>.</decimalDelimitor_en>
      <decimalDelimitor_fr>,</decimalDelimitor_fr>
      <msgConfigServiceType>AWDINT</msgConfigServiceType>
      <awdWorkType>IFASTREQ</awdWorkType>
    </SmartPDFConfig>
  </IFDS>
</Server>

