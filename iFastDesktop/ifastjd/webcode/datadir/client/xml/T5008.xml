			<!-- Transactions TABLE -->
			<control>
				<position>absolute</position>
				<type>table</type>
				<description>Date</description>
				<top>248px</top>
				<left>4px</left>
				<height/>
				<width/>
				<viewMapping>//TaxFormDtls/TaxFund</viewMapping>
				<tableFor>transactions</tableFor>
				<columnWidths>
					<col name="fundName" width="2.4in" header-text-align="center" data-text-align="left" font-size="10px" font="Helvetica"  />
					<col name="foreignCurrency" width="1in" header-text-align="center" data-text-align="right" font="Helvetica" font-size="10px"/>
					<col name="strMMDD" width="0.75in" header-text-align="center" data-text-align="center" font="Helvetica" font-size="10px"/>
					<col name="securityCode" width="1.25in" header-text-align="center" data-text-align="center" font-size="10px" font="Helvetica" />
					<col name="quantityOfSeq" width="1in" header-text-align="center" data-text-align="right" font="Helvetica" font-size="10px"/>
					<col name="proceedsOfDisposition" width="1in" header-text-align="center" data-text-align="right" font="Helvetica" font-size="10px"/>
					<col name="costOfBookValue" width="1in" header-text-align="center" data-text-align="right" font="Helvetica" font-size="10px"/>
