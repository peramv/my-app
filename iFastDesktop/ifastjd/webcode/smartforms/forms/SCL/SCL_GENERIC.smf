			IFDS_GENERIC.onRenderSWPControl = function(control)
			{				
				SmartFormsApp.DataStore.getNode('gnAcctSetupRequest.TEMP.acctType').addListener('updated',
					function(node){
						var acctTypeParts = node.getValue().split("_");				
						SmartFormsApp.DataStore.setNodeValue('gnAcctSetupRequest.AcctSetup.AcctInfo.acctType', acctTypeParts[0]);
						FormItems.Util.toggleControlVisible(this, acctTypeParts[0] == '01')
						if (node.getValue() != '01')
						{
							SmartFormsApp.DataStore.getNode('gnPACSWPRequest[2]').clearData();
							SmartFormsApp.DataStore.getNode('gnPACSWPRequest[3]').clearData();
						}
					}, control);				
			}					
			
			IFDS_GENERIC.onRenderRIFControl = function(control)
			{
				control.hide();				
				SmartFormsApp.DataStore.getNode('gnAcctSetupRequest.TEMP.acctType').addListener('updated',
					function(node){
						var acctTypeParts = node.getValue().split("_");				
						SmartFormsApp.DataStore.setNodeValue('gnAcctSetupRequest.AcctSetup.AcctInfo.acctType', acctTypeParts[0]);
						FormItems.Util.toggleControlVisible(this, node.getValue() != '01')
						if (node.getValue() == '01')
						{
							SmartFormsApp.DataStore.getNode('gnLIFRRIFRequest[0]').clearData();
							SmartFormsApp.DataStore.getNode('gnLIFRRIFRequest[1]').clearData();
						}
					}, control);				
			}
			
