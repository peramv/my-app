    <build>
        <compiler>vc71</compiler>
        <workspace>ifastgui</workspace>
        <project>ifastgui</project>
        <branderproject>ifastcbo</branderproject>
        <brandername>ifast</brandername>
        <branderresourcefile><project>ifastgui</project><file>ifastgui.rc</file></branderresourcefile>
        <branderresourcefile><project>ifastgui/rescommon</project><file>rescommon.rc</file></branderresourcefile>
        <branderresourcefile><project>ifastgui/rescanada</project><file>rescanada.rc</file></branderresourcefile>
        <branderresourcefile><project>ifastgui/reseurope</project><file>reseurope.rc</file></branderresourcefile>
        <branderresourcefile><project>ifastgui/rescommonfr</project><file>rescommonfr.rc</file></branderresourcefile>
        <branderresourcefile><project>ifastgui/rescanadafr</project><file>rescanadafr.rc</file></branderresourcefile>		
        <branderresourcefile><project>dataimpl</project><file>ifast_dataimpl.rc</file></branderresourcefile>
        <branderresourcefile><project>ifastcbo</project><file>ifastcbo.rc</file></branderresourcefile>
        <branderresourcefile><project>ifastbp</project><file>ifastbp.rc</file></branderresourcefile>
        <branderresourcefile><project>ifastdbrkr</project><file>ifastdbrkr.rc</file></branderresourcefile>
        <branderresourcefile><project>uibase</project><file>popupmessagedialog.rc</file></branderresourcefile>
        <BranderConditionFile><project>data</project><file>ifastcbo.cnd</file></BranderConditionFile>
        <BranderConditionFile><project>data</project><file>ifastbp.cnd</file></BranderConditionFile>
        <BranderConditionFile><project>data</project><file>ifastdbrkr.cnd</file></BranderConditionFile>
        <custom>build_ifast_desktop.pl</custom>
    </build>
    <documentation>
    </documentation>
    <distribution>
        <basename>ifast_desktop</basename>
        <debug>
            <relocationlist>ifast_desktop.debug.manifest</relocationlist>
            <distribution>ifastlocal</distribution>
        </debug>
        <release>
            <relocationlist>ifast_desktop.release.manifest</relocationlist>
            <!-- Don't bother to distribute anything for release mode
            <distribution>ifastlocal</distribution>
            -->
            <!-- Release mode produces an InstallShield image instead -->
            <installshield>
                <isversion>9</isversion>
                <project>Desktop</project>
                <projectFile>DST Graphical Front End.ism</projectFile>
                <media>Default</media>
                <targetdir>InstallShield/Desktop/Media/Default</targetdir>
                <relocationlist>ifast_desktop.cdimage.relocationlist</relocationlist>
                <postrelocationlist>ifast_desktop.cdimage.post.relocationlist</postrelocationlist>
                <filegroup>
                    <file>File Groups/Program Executable Files.fgl</file>
                    <fromtext>\\stage\\bin\\</fromtext>
                    <totext>bin\release\</totext>
                </filegroup>
                <filegroup>
                    <file>File Groups/Program Executable Files.fgl</file>
                    <fromtext>\\stage\\csi\\</fromtext>
                    <totext>ifastcsi\</totext>
                </filegroup>
                <filegroup><!-- Must do Data.fgl twice for stage/data/config and stage/data -->
                    <file>File Groups/Data.fgl</file>
                    <fromtext>[/\\]stage[/\\]data[/\\]config[/\\]</fromtext>
                    <totext>data\config\</totext>
                </filegroup>
                <filegroup>
                    <file>File Groups/Data.fgl</file>
                    <fromtext>[/\\]stage[/\\]data[/\\]</fromtext>
                    <totext>data\</totext>
                </filegroup>
                <filegroup>
                    <file>File Groups/Attachmate.fgl</file>
                    <fromtext>[/\\]stage[/\\]csi[/\\]</fromtext>
                    <totext>ifastcsi\</totext>
                </filegroup>
                <filegroup>
                    <file>File Groups/r2wEN.fgl</file>
                    <fromtext>[/\\]stage[/\\]csi[/\\]</fromtext>
                    <totext>ifastcsi\</totext>
                </filegroup>
                <filegroup>
                    <file>File Groups/r2wJP.fgl</file>
                    <fromtext>[/\\]stage[/\\]csi[/\\]</fromtext>
                    <totext>ifastcsi\</totext>
                </filegroup>
                <client>
                    <clientname>TA</clientname>
                    <packagetype>zip</packagetype>
                </client>
                <client>
                    <clientname>AIM</clientname>
                    <packagetype>zip</packagetype>
                </client>
                <client>
                    <clientname>EP</clientname>
                    <packagetype>zip</packagetype>
                </client>
                <client>
                    <clientname>Dynamic</clientname>
                    <packagetype>zip</packagetype>
                </client>
                <client>
                    <clientname>Manulife</clientname>
                    <packagetype>zip</packagetype>
                </client>
                <client>
                    <clientname>SSB</clientname>
                    <packagetype>zip</packagetype>
                </client>
            </installshield>
        </release>
    </distribution>
</ifast_desktop>

