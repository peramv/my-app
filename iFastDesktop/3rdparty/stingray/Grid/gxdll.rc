/////////////////////////////////////////////////////////////////////////////
// Version stamp for this .DLL

// NOTE: MSVC 6.0 deprecated ver.h by way of the ver.h file, i.e. ver.h simply 
//       includes winver.h. As of Visual Studio 2003, Microsoft has done away 
//       with ver.h altogether, so just including winver.h is sufficient.
#include "winver.h"

#define GXRELEASE

// Version Info for this .DLL
#ifndef __PRODUCT_VERSION__H__
#include <ProductVersions.h>
#endif // __PRODUCT_VERSION__H__

VS_VERSION_INFO     VERSIONINFO
FILEVERSION	    _GX_VER_COMMA
PRODUCTVERSION      _GX_VER_COMMA
FILEFLAGSMASK       VS_FFI_FILEFLAGSMASK
#ifndef GXRELEASE
#ifdef _DEBUG
FILEFLAGS           VS_FF_DEBUG|VS_FF_PRERELEASE
#else
FILEFLAGS           VS_FF_PRERELEASE
#endif
#else
#ifdef _DEBUG
FILEFLAGS           VS_FF_DEBUG
#else
FILEFLAGS           0 // final release
#endif
#endif
#ifdef _WIN32
FILEOS              VOS_NT_WINDOWS32
#else
FILEOS              VOS_DOS_WINDOWS16
#endif
FILETYPE            VFT_DLL
FILESUBTYPE         0   // not used

BEGIN
	BLOCK "StringFileInfo"
	BEGIN
		BLOCK "040904E4" // Lang=US English, CharSet=Windows Multilual
		BEGIN

			VALUE "CompanyName",     "Rogue Wave Software\0"
			VALUE "FileVersion",	 _GX_VER_TEXT
			VALUE "LegalCopyright",  "Copyright (c) 1997-2012 Rogue Wave Software, Inc.  All Rights Reserved."

#ifdef _DEBUG
			VALUE "FileDescription", "ObjGrid DLL Shared Library - Debug Version\0"
			VALUE "InternalName",    "GXDLLD\0"
			VALUE "OriginalFilename","GX"_GX_VER_LIB"D.DLL\0"
#else // !_DEBUG
			VALUE "FileDescription", "ObjGrid DLL Shared Library - Release Version\0"
			VALUE "InternalName",    "GXDLLR\0"
			VALUE "OriginalFilename","GX"_GX_VER_LIB"R.DLL\0"
#endif
			VALUE "ProductName",     "Objective Grid\0"
			VALUE "ProductVersion",  _GX_VER_TEXT
		END
	END
	BLOCK "VarFileInfo"
	BEGIN
		VALUE "Translation", 0x409, 1252
			// English language (0x409) and the Windows ANSI codepage (1252)
	END
END

